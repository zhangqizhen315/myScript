{
    "title": "这是 title 的测试",
    "author": "作者: 张启震",
    "description": "这是一个测试用的配置文件",
    "gitbook": "3.2.3",
    "language": "zh-hans",
    "root": ".",
    "plugins": [
        "-search",
        "-lunr",
        "search-plus",
        "-highlight",
        "prism",
        "katex",
        "mermaid-gb3",
        "puml",
